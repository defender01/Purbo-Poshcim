        <div class="main-container">
            <div class="title">
                আমাদের সদস্যগন
            </div>
            <div class="staff-container">
                <% for(let i=0; i< staffs.names.length; i++){ %>
                <div class="staff">
                    <div class="img-container">
                        <div class="img-holder">
                            <img src="/assets/images/staff/<%=staffs.imgFiles[i]%>" alt="Image not found">
                        </div>
                    </div>
                    <div class="name">
                        <%=staffs.names[i]%>
                    </div>
                    <div class="role">
                      <%=staffs.roles[i]%>
                    </div>
                </div>
                <% } %>
            </div>
           
        </div>   
      </div>
    </div>

    <% include ./segments/footer.ejs %>

